@model IEnumerable<FootBall2.FootBallLeague>
    @using FootBall2.Models
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<body align="center" style="background-color:aquamarine;">
    <div>
        <h1 align="center"><b>FootBall League</b></h1>
    </div>
    <div>
        <h3 align="center"><b><i>Football Match Summary Details</i></b></h3>
    </div>
    <div align="center">


        @foreach (var item in Model)
        {
            <h2>MatchId : @item.MatchID</h2>
            <h3>TeamName 1 : @item.TeamName1</h3>
            <h3>TeamName 2 : @item.TeamName2</h3>
            <h3>Match Status : @item.Status</h3>
            <h3>Winning Team : @item.WinningTeam</h3>
            <h3>Points : @item.Points</h3>
            <hr />


        }



    </div>
</body>